    "ember-cli-version-checker": "~2.0.0",
    "ember-computed-decorators": "0.3.0",
    "ember-concurrency": "0.7.19",
    "ember-elsewhere": "1.0.1",
    "ember-get-config": "^0.2.4 ",
    "ember-hook": "1.4.2",
    "ember-prop-types": "^7.0.2",
    "ember-spread": "^5.0.0",
    "ember-truth-helpers": "^1.3.0"
  },
  "pr-bumper": {
    "coverage": 94.22
  }
}
