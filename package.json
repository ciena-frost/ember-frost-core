    "ember-source": "~2.12.0",
    "ember-spread": "^1.2.2",
    "ember-test-utils": "^4.0.0",
    "ember-truth-helpers": "^1.3.0",
    "frost-guide-custom-routing": "0.0.4",
    "loader.js": "^4.2.3",
    "markdown-code-highlighting": "0.1.0",
    "sinon-chai": "^2.8.0"
